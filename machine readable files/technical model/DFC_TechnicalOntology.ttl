@prefix : <http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl> .

<http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl#hasPivot
:hasPivot rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl#hostedBy
:hostedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl#represent
:represent rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl#Platform
:Platform rdf:type owl:Class ;
          rdfs:label "Organisation qui heberge la donnée"@fr ,
                     "Plateforme"@fr .


###  http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl#RepresentationPivot
:RepresentationPivot rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty :represent ;
                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :RepresentedThing
                                     ] ;
                     rdfs:comment "Permet de designer tous les RepresentatedThing qui sont équivalents et d'etre désigné par un RepresentedThing pour connaitre ses équivalence par transitivité"@fr ;
                     rdfs:label "Pivot de représentation"@fr .


###  http://datafoodconsortium.org/ontologies/DFC_TechnicalOntology.owl#RepresentedThing
:RepresentedThing rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasPivot ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :RepresentationPivot
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hostedBy ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Platform
                                  ] ;
                  rdfs:comment "Chose représentée sur une platefome posadant des equivalences sur d'autres plateformes"@fr ;
                  rdfs:label "Chose représentée"@fr .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Platform
                :RepresentationPivot
                :RepresentedThing
              )
] .


